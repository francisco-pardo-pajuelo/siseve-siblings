	keywords = {Spillovers, conformism, policies, structural estimation},
	pages = {543--565},
	file = {Full Text PDF:C\:\\Users\\Francisco\\Zotero\\storage\\SZVW59R2\\Boucher et al. - 2024 - Toward a General Theory of Peer Effects.pdf:application/pdf;Snapshot:C\:\\Users\\Francisco\\Zotero\\storage\\VL75FNHG\\ECTA21048.html:text/html},
