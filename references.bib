@incollection{murphy_implications_2023,
	title = {The {Implications} of {Pupil} {Rank} for {Achievement}},
	isbn = {978-0-19-062597-9},
	url = {https://oxfordre.com/economics/view/10.1093/acrefore/9780190625979.001.0001/acrefore-9780190625979-e-845},
	abstract = {The significance of social interaction has become an increasingly important part of economic thought and models through the work on peer effects, social norms, and networks. Within this literature, a novel focus of ranking within groups has emerged. The rank of an individual is usually defined as the ordinal position within a specific group. This could be the work environment or a classroom, and much of this literature focuses on rank effects in education settings. The literature studies rank effects for various age groups. There is evidence that a rank position even during early life phases, such as in elementary education, has lasting effects on education outcomes such as test scores or subject specializations, choices during college, and wages. A first-order challenge in the study of rank effects is to separate them from other highly correlated effects. For example, individuals with a high rank academic rank in a group will likely have high academic ability in absolute terms. Papers in this field directly account for measured ability, and so rely on the variation in rank that exists across groups for any given ability measure, that is, a score of 80 in one group would rank the student top, while near the bottom in another. The comparability of achievement measures across settings is key; one commonly employed solution is to account for level differences across settings. While the literature has now established the importance of rank, there are several—potentially noncompeting—ideas for the precise behavioral mechanisms of why rank matters so much. Future work will most likely focus on integrating rank effects into the literature on social interactions to discuss implications for optimal group formation.},
	language = {en},
	urldate = {2024-04-04},
	booktitle = {Oxford {Research} {Encyclopedia} of {Economics} and {Finance}},
	publisher = {Oxford University Press},
	author = {Murphy, Richard and Weinhardt, Felix},
	collaborator = {Murphy, Richard and Weinhardt, Felix},
	month = oct,
	year = {2023},
	doi = {10.1093/acrefore/9780190625979.013.845},
	file = {Murphy and Weinhardt - 2023 - The Implications of Pupil Rank for Achievement.pdf:C\:\\Users\\Francisco\\Zotero\\storage\\EM566REZ\\Murphy and Weinhardt - 2023 - The Implications of Pupil Rank for Achievement.pdf:application/pdf},
}

@article{mountjoy_marginal_2024,
