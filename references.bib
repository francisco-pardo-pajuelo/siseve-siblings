	keywords = {Causal inference, conditional randomization test, equivariance, exact p-value, non-sharp null hypothesis},
	pages = {567--590},
	file = {Full Text PDF:C\:\\Users\\Francisco\\Zotero\\storage\\VDEIGRUS\\Basse et al. - 2024 - Randomization Tests for Peer Effects in Group Form.pdf:application/pdf;Snapshot:C\:\\Users\\Francisco\\Zotero\\storage\\UZJTW6U8\\ECTA20134.html:text/html},
